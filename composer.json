{
    "name": "danielburger1337/oauth2-pkce",
    "type": "library",
    "description": "Create/Verify OAuth2 PKCE code challenges.",
    "license": "MIT",
    "homepage": "https://github.com/danielburger1337/oauth2-pkce-php",
    "authors": [
        {
            "name": "Daniel Burger",
            "homepage": "https://github.com/danielburger1337"
        }
    ],
    "keywords": [
        "oauth2",
        "pkce"
    ],
    "require": {
        "php": "^8.2",
        "spomky-labs/base64url": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.66",
        "phpstan/phpstan": "^1.12.13",
        "phpunit/phpunit": "^11.5.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "danielburger1337\\OAuth2\\PKCE\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "danielburger1337\\OAuth2\\PKCE\\Tests\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}
